@using AV.AvA.Model
<MudList Clickable="true">
	@foreach(var pv in PersonVersions ?? Enumerable.Empty<PersonVersion>())
	{
		var href = $"/Details/{pv.AvId}";
		var p = pv.Person;
		<MudListItem Href="@href"Icon="@Icons.Material.Filled.AccountCircle">
			@p.Vorname
			@p.Zweitnamen
			@p.NachnamePraefix
			@p.Nachname
		</MudListItem>
	}
</MudList>

@code {
	[Parameter]
	public IReadOnlyCollection<PersonVersion>? PersonVersions { get; set; }
}
